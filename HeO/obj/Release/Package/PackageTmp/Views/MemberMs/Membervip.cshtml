@{ 
    var Viprecord = (IEnumerable<Viprecord>)ViewBag.Viprecord;
}
<main id="main">
    <section class="member block_container">
        <div class="block_inner">
            @{ Html.RenderPartial("_Member"); }
            <div class="member_container">
                <div class="content-block">
                    <h1>VIP會員</h1>
                    <div class="buy-vip-wrap">
                        <h2>剩餘天數<span class="font-purple"> @ViewBag.Date</span></h2><a class="btn" href="@Url.Action("Deposit", "DepositMs")" title="購買VIP">購買VIP</a>
                    </div>
                    <div class="text">購買HeO平台的月制VIP後，使用冷卻會大幅度下降，並且HeO承諾您的帳號不會加入互惠被其他人使用，至於開放買家購買您帳號服務您也可以自由選擇開放或關閉</div>
                </div>
                <div class="content-block">
                    <div class="content-block-title">VIP紀錄列表</div>
                    <div class="stored_history">
                        <div class="history">
                            <div class="rwd_table_wrapper">
                                <table>
                                    <thead>
                                        <tr>
                                            <th>購買日期</th>
                                            <th>到期日期</th>
                                            <th>購買金額</th>
                                            <th>使用天數</th>
                                            <th>備註</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (Viprecord viprecordList in Viprecord)
                                        {
                                            <tr>
                                                <td class="align_left">2019/03/10</td>
                                                <td class="align_left">@viprecordList.Enddate.ToString("yyyy/MM/dd")</td>
                                                <td>@viprecordList.Day</td>
                                                <td>@viprecordList.Money</td>
                                                <td></td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>