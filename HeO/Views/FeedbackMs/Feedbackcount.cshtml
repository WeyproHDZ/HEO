@{
    var Feedbackproduct = (IEnumerable<Feedbackproduct>)ViewBag.Feedbackproduct;
    int i = 0;
}
<main id="main">
    <section class="member block_container">
        <div class="block_inner">
            @{ Html.RenderPartial("_Member"); }
            <div class="member_container">
                <div class="content-block">
                    <h1>使用次數紀錄</h1>
                    <h2>加入HeO的時間<span class="font-purple"> @ViewBag.HeODate</span></h2>
                    <div class="text">當您加入HeO並開啟授權後，我們將紀錄該帳號的總服務數量與相對應的回饋獎勵金。</div>
                    @foreach (Feedbackproduct FeedbackproductList in Feedbackproduct)
                    {
                        <div class="main-content">
                            <div class="transfer-number caculate">
                                <p>授權 @FeedbackproductList.Feedbackproductname 使用次數<span class="font-purple">@ViewBag.FeedbackCount[i]</span></p>
                                <p class="padding">x</p>
                                <p>獎勵金 @FeedbackproductList.Feedbackdetail.FirstOrDefault().Money $</p>
                                <p class="padding">=</p>
                                <p>總累積獎勵金<span class="font-purple">@(ViewBag.FeedbackCount[i] * FeedbackproductList.Feedbackdetail.FirstOrDefault().Money) $</span></p>
                            </div>
                        </div>
                        i++;
                    }
                </div>
            </div>
        </div>
    </section>
</main>